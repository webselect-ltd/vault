{
    "version": "1.0.0",
    "name": "vault",
    "private": true,
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.8",
        "@types/bootstrap": "^5.2.10",
        "@types/jest": "^29.5.14",
        "@types/js-cookie": "^3.0.6",
        "autoprefixer": "^10.4.20",
        "clean-webpack-plugin": "^4.0.0",
        "cssnano": "^7.0.6",
        "del-cli": "^6.0.0",
        "fast-text-encoding": "^1.0.6",
        "gulp": "^5.0.0",
        "gulp-postcss": "^10.0.0",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^6.0.0",
        "gulp-sourcemaps": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.49",
        "sass": "^1.83.0",
        "source-map-loader": "^5.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2",
        "webpack": "^5.97.1",
        "webpack-cli": "^6.0.1"
    },
    "dependencies": {
        "bootstrap": "^5.3.3",
        "bootstrap-icons": "^1.11.3",
        "clipboard": "^2.0.11",
        "handlebars": "^4.7.8",
        "mab-bootstrap-taginput": "^0.8.5",
        "mab-dom": "^0.12.2",
        "tslib": "^2.8.1"
    },
    "scripts": {
        "js:clean": "del-cli ./wwwroot/js/dist",
        "js:build": "webpack --mode=production",
        "css:clean": "del-cli ./wwwroot/css/dist",
        "css:build": "gulp build-css",
        "build": "run-s css:clean css:build js:clean js:build",
        "watch-js": "webpack --watch --mode=production",
        "test": "jest",
        "coverage": "jest --coverage"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "jsdom",
        "testPathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ],
        "coveragePathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ]
    },
    "-vs-binding": {
        "ProjectOpened": [
            "watch-js"
        ]
    }
}
