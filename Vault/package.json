{
    "version": "1.0.0",
    "name": "vault",
    "private": true,
    "devDependencies": {
        "@tsconfig/recommended": "^1.0.2",
        "@types/bootstrap": "^5.2.6",
        "@types/jest": "^29.5.3",
        "@types/js-cookie": "^3.0.3",
        "autoprefixer": "^10.4.14",
        "clean-webpack-plugin": "^4.0.0",
        "cssnano": "^6.0.1",
        "del-cli": "^5.0.0",
        "fast-text-encoding": "^1.0.6",
        "gulp": "^4.0.2",
        "gulp-postcss": "^9.0.1",
        "gulp-rename": "^2.0.0",
        "gulp-sass": "^5.1.0",
        "gulp-sourcemaps": "^3.0.0",
        "jest": "^29.6.1",
        "jest-environment-jsdom": "^29.6.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.25",
        "sass": "^1.63.6",
        "source-map-loader": "^4.0.1",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "webpack": "^5.88.1",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "bootstrap": "^5.3.0",
        "bootstrap-icons": "^1.10.5",
        "clipboard": "^2.0.11",
        "handlebars": "^4.7.7",
        "mab-dom": "^0.12.1",
        "tslib": "^2.6.0"
    },
    "scripts": {
        "js:clean": "del-cli ./wwwroot/js/dist",
        "js:build": "webpack --mode=production",
        "css:clean": "del-cli ./wwwroot/css/dist",
        "css:build": "gulp build-css",
        "build": "run-s css:clean css:build js:clean js:build",
        "watch-js": "webpack --watch --mode=production",
        "test": "jest",
        "coverage": "jest --coverage"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "jsdom",
        "testPathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ],
        "coveragePathIgnorePatterns": [
            "bin/",
            "obj/",
            "/node_modules/",
            "/legacy/",
            "Fake.*",
            "all.ts"
        ]
    },
    "-vs-binding": {
        "ProjectOpened": [
            "watch-js"
        ]
    }
}
